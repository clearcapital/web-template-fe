{
  "name": "fe-template",
  "version": "1.0.0",
  "description": "this is a front end template using react 15, redux, and webpack 2 built by jordan with love",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "postinstall": "rm -rf bin; npm run build",
    "dev": "./node_modules/.bin/webpack-dev-server --config ./build/webpack.dev.js --progress --no-info --port 3001",
    "build": "./node_modules/.bin/webpack --config ./build/webpack.prod.js --progress --colors",
    "deploy": "sh ./build/build-deploy.sh",
    "pretest": "npm run test:lint",
    "test": "./node_modules/.bin/jest",
    "posttest": "npm run test:coverage",
    "test:watch": "./node_modules/.bin/jest --watchAll --verbose",
    "test:lint": "./node_modules/.bin/eslint . --color; exit 0",
    "test:coverage": "echo 'todo: coverage tests'",
    "purge": "rm -rf node_modules; rm -rf bin; npm cache clean; npm install"
  },
  "author": "Clear Capital: Jordan Papaleo",
  "license": "MIT",
  "engines": {
    "node": "6.3.1",
    "npm": "3.10.7"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "<rootDir>/tests/StyleStub.js",
      "^.+\\.(jpg|png|jpeg|svg)$": "<rootDir>/tests/MediaStub.js"
    },
    "collectCoverage": true
  },
  "dependencies": {
    "axios": "0.14.0",
    "basic-auth": "1.0.4",
    "classnames": "2.2.5",
    "express": "4.14.0",
    "gsap": "1.19.0",
    "lodash": "4.16.1",
    "prop-types": "^15.5.10",
    "react": "15.4.1",
    "react-addons-update": "15.4.1",
    "react-dom": "15.4.1",
    "react-redux": "4.4.5",
    "react-router": "2.8.0",
    "react-router-redux": "4.0.5",
    "redux": "3.6.0",
    "redux-multi": "0.1.12",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.1.0"
  },
  "devDependencies": {
    "babel-core": "6.14.0",
    "babel-eslint": "7.0.0",
    "babel-jest": "15.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-class-properties": "6.11.5",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-1": "6.13.0",
    "babel-register": "6.14.0",
    "conventional-changelog": "1.1.0",
    "css-loader": "0.25.0",
    "enzyme": "2.4.1",
    "eslint": "3.5.0",
    "eslint-config-standard": "6.0.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-react": "6.2.0",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.9.0",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "jest": "17.0.3",
    "jest-cli": "17.0.3",
    "json-loader": "0.5.4",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "moxios": "0.3.0",
    "node-sass": "3.9.3",
    "normalize-css": "2.3.1",
    "raw-loader": "0.5.1",
    "react-hot-loader": "3.0.0-beta.7",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.1.1",
    "sass-loader": "4.1.1",
    "standard": "8.0.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
}
